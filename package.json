{
  "name": "@gregoriusrippenstein/erlang-red-unittest",
  "version": "0.9.3",
  "dependencies": {
  },

  "description": "Unit testing nodes for Erlang-RED.",
  
  "keywords": [ 
    "node-red", 
    "erlang-red", 
    "unit-testing",
    "assert-the-truth" 
  ],

  "license": "Don't do evil.",
  "author": "Gerrit Riessen <gerrit@flowhub.org>",
  "engines": {
    "node": ">=14"
  },

  "node-red" : {
    "version": ">=3.0.0",

    "plugins": {
      "unittesting_sidebar": "plugins/sidebar.html"
    },
    
    "nodes": {
      "ut-assert-failure": "nodes/ut-assert-failure.js",
      "ut-assert-values": "nodes/ut-assert-values.js",
      "ut-assert-success": "nodes/ut-assert-success.js",
      "ut-assert-status": "nodes/ut-assert-status.js",
      "ut-assert-debug": "nodes/ut-assert-debug.js"
    }
  }
}
